{
  "name": "orchestra/notifier",
  "description": "User Notification Component for Orchestra Platform",
  "keywords": [
    "orchestra-platform",
    "orchestral",
    "laravel",
    "mailer"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mior Muhammad Zaki",
      "email": "crynobone@gmail.com",
      "homepage": "https://github.com/crynobone"
    }
  ],
  "autoload": {
    "psr-4": {
      "Orchestra\\Notifier\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Orchestra\\Notifier\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "illuminate/encryption": "^9.0",
    "illuminate/mail": "^9.0",
    "illuminate/queue": "*",
    "laravel/serializable-closure": "^1.0@dev",
    "opis/closure": "^3.4.2",
    "orchestra/contracts": "7.x-dev",
    "orchestra/memory": "7.x-dev",
    "orchestra/support": "7.x-dev",
    "symfony/http-client": "7.2.x-dev",
    "symfony/mailer": "^6.0",
    "symfony/mailgun-mailer": "7.1.x-dev",
    "symfony/mime": "^6.0",
    "tijsverkoyen/css-to-inline-styles": "^2.2.1"
  },
  "require-dev": {
    "aws/aws-sdk-php": "^3.0",
    "guzzlehttp/guzzle": "^6.5.5 || ^7.0.1",
    "orchestra/testbench": "^7.0"
  },
  "suggest": {
    "aws/aws-sdk-php": "Required to use the SES mail driver (^3.0).",
    "guzzlehttp/guzzle": "Required to use the Mailgun mail drivers (^6.5.5 || ^7.0.1)."
  },
  "extra": {
    "branch-alias": {
      "dev-master": "7.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/katsana-external/contracts"
    },
    {
      "type": "vcs",
      "url": "https://github.com/katsana-external/support"
    },
    {
      "type": "vcs",
      "url": "https://github.com/katsana-external/memory"
    }
  ]
}
